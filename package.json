{
  "name": "san-sei-website",
  "version": "1.0.0",
  "description": "San-Sei delivery website project",
  "main": "./server/server.ts",
  "scripts": {
    "start-dev": "nodemon ./server/server.ts",
    "dev": "webpack serve --mode development --config client/webpack.config.js",
    "build": "webpack --mode production --progress --config client/webpack.config.js",
    "tscheck": "tsc",
    "test": "jest && npx eslint client/src/*",
    "jest": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 3
          }
        ],
        "@babel/preset-react"
    ]
  },
  "author": "Nikita Leontev",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.5.0",
    "libphonenumber-js": "^1.10.19",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.7.0",
    "react-slideshow-image": "^4.2.0",
    "react-snaplist-carousel": "^4.4.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "core-js": "^3.27.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-storybook": "^0.6.10",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.3",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.11",
    "style-loader": "^3.3.1",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
